    "@sparticuz/chromium": "^141.0.0",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "autoprefixer": "^10.0.1",
    "axios": "^1.6.2",
    "chart.js": "^4.5.1",
    "date-fns": "^2.30.0",
    "framer-motion": "^10.16.16",
